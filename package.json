{
  "name": "csv-management-app",
  "version": "1.0.0",
  "description": "Full-stack CSV data management application",
  "main": "backend/src/app.js",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test -- --watchAll=false",
    "test:all": "npm run test:backend && npm run test:frontend",
    "test:coverage": "npm run test:backend -- --coverage && npm run test:frontend -- --coverage",
    "start": "cd backend && npm start",
    "docker:build": "docker build -t csv-management-app .",
    "docker:run": "docker run -p 3001:3001 csv-management-app",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose --profile production up --build",
    "render:build": "cd backend && npm ci --only=production && cd ../frontend && npm ci && npm run build && mv build ../backend/public",
    "render:start": "cd backend && npm start",
    "lint:backend": "cd backend && npx eslint src/",
    "lint:frontend": "cd frontend && npx eslint src/",
    "lint": "npm run lint:backend && npm run lint:frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "csv",
    "data-management",
    "react",
    "nodejs",
    "express",
    "validation",
    "file-upload"
  ],
  "author": "CSV Management Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/csv-management-app.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/csv-management-app/issues"
  },
  "homepage": "https://github.com/your-username/csv-management-app#readme",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.12.2",
    "form-data": "^4.0.4"
  }
}
